{
  "name": "unprunk",
  "version": "0.1.1",
  "description": "What the funk is a prunk?",
  "main": "build/index.js",
  "types": "types",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "jest --coverage && npm run tslint",
    "watch": "jest --watch",
    "build": "tsc",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "version": "npm run build && conventional-changelog -p eslint -i CHANGELOG.md -s -r 0 && git add -A",
    "bump": "npm version --unsafe-perm $(conventional-recommended-bump -p eslint)",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustinLivi/prunk.git"
  },
  "keywords": [
    "thunk",
    "config",
    "property"
  ],
  "author": "Justin Livi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JustinLivi/prunk/issues"
  },
  "homepage": "https://github.com/JustinLivi/prunk#readme",
  "dependencies": {
    "@types/lodash": "4.14.151",
    "lodash": "4.17.20",
    "snyk": "^1.685.0"
  },
  "devDependencies": {
    "@types/node": "13.9.1",
    "@types/jest": "24.0.15",
    "conventional-changelog-cli": "2.0.21",
    "conventional-changelog-eslint": "3.0.1",
    "conventional-recommended-bump": "5.0.0",
    "coveralls": "3.0.5",
    "jest": "24.1.0",
    "jest-extended": "0.11.2",
    "prettier": "1.18.0",
    "ts-jest": "24.0.0",
    "tslint": "5.13.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.3.3333"
  },
  "snyk": true
}
